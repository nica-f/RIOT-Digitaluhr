config BOARD
    default "feather-nrf52840-sense" if BOARD_FEATHER_NRF52840_SENSE

config BOARD_FEATHER_NRF52840_SENSE
    bool
    default y
    select BOARD_COMMON_NRF52
    select CPU_MODEL_NRF52840XXAA

source "$(RIOTBOARD)/common/nrf52/Kconfig"
